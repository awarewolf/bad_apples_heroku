<div class="row">
  <div class="col-md-3">
  </div>
  <div class="col-md-6">
    <h1>Log In</h1>
    <%= bootstrap_form_for(sessions_path) do |f| %>
      <%= f.email_field :email %>
      <%= f.password_field :password %>
      <% f.check_box :remember_me %>
      <%= f.submit "Log in" %>
    <% end %>
  </div>
  <div class="col-md-3">
  </div>
</div>
